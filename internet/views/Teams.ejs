<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
            crossorigin="anonymous"></script>
</head>
<body>
<a href="/" button type="button" class="btn btn-primary position-absolute top-10 start-50"> Home</a>
</button>

<img src="/images/logo.png" style="height: 150px">
<h1>Teams main</h1>


<nav>
    <div class="nav nav-tabs" id="nav-tab" role="tablist">
        <button class="nav-link active" id="nav-west-tab" data-bs-toggle="tab" data-bs-target="#nav-west" type="button"
                role="tab" aria-controls="nav-west" aria-selected="true">Western
        </button>
        <button class="nav-link" id="nav-east-tab" data-bs-toggle="tab" data-bs-target="#nav-east" type="button"
                role="tab" aria-controls="nav-east" aria-selected="false">Eastern
        </button>
    </div>
</nav>
<div class="tab-content" id="nav-tabContent">
    <div class="tab-pane fade show active" id="nav-west" role="tabpanel" aria-labelledby="nav-west-tab">

        <div class="container px-2" style="margin: 10px">


            <div class="row row-cols-5 gx-2  row-cols-lg-3">
                <div class="col">
                    <div class="p-1 border bg-light" style="text-align: center">Atlantic</div>
                </div>
                <div class="col">
                    <div class="p-1 border bg-light" style="text-align: center">Central</div>
                </div>
                <div class="col">
                    <div class="p-1 border bg-light" style="text-align: center">Souteast</div>
                </div>

                <% for (let team of teams ) { %>
                    <div class="col" >
                        <div class="p-3  border bg-white">


                            <div class="container">
                                <div class="row row-cols-auto">
                                    <div class="col">
                                        <a href="/detail/<%= team.id %>"><img src="images/<%= team.Logo %>.jpg" style="height: 75px"></a>
                                    </div>
                                    <div class="col-9">

                                        <div class="container">
                                            <div class="row row-cols-1">
                                                <div class="col">
                                                    <a href="/detail"
                                                       style="text-decoration:none; color:black "><%= team.Name %></a>
                                                </div>
                                                <div class="col ">

                                                    <div class="container">
                                                        <div class="row row-cols-3">
                                                            <div class="col">
                                                                <a href="/detail"
                                                                   style="text-decoration:none; color:black">Roster</a>
                                                            </div>
                                                            <div class="col">
                                                                Matchup

                                                            </div>
                                                            <div class="col">First lineup</div>
                                                        </div>
                                                    </div>

                                                </div>

                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>

                        </div>
                    </div>

                <% } %>
            </div>


        </div>

    </div>
    <div class="tab-pane fade" id="nav-east" role="tabpanel" aria-labelledby="nav-east-tab">

        <div class="container px-2">


            <div class="row row-cols-5 gx-2  row-cols-lg-3">
                <div class="col">
                    <div class="p-1 border bg-light">Northwest</div>
                </div>
                <div class="col">
                    <div class="p-1 border bg-light">Pacific</div>
                </div>
                <div class="col">
                    <div class="p-1 border bg-light">Southwest</div>
                </div>
                <div class="col">
                    <div class="p-3  border bg-white">

                        <div class="container">
                            <div class="row row-cols-auto">
                                <div class="col">
                                    <a href="/detail"><img src="/images/lo.jpg" style="height: 75px"></a>
                                </div>
                                <div class="col-9">

                                    <div class="container">
                                        <div class="row row-cols-1">
                                            <div class="col">
                                                <a href="/detail" style="text-decoration:none; color:black">Team
                                                    name</a>
                                            </div>
                                            <div class="col ">

                                                <div class="container">
                                                    <div class="row row-cols-3">
                                                        <div class="col">
                                                            <a href="/detail" style="text-decoration:none; color:black">Roster</a>
                                                        </div>
                                                        <div class="col">
                                                            Matchup

                                                        </div>
                                                        <div class="col">First lineup</div>
                                                    </div>
                                                </div>

                                            </div>

                                        </div>
                                    </div>

                                </div>

                            </div>
                        </div>

                    </div>
                </div>


                    </div>
                </div>


            </div>


        </div>


    </div>
</div>

</div>

</body>
</html>
