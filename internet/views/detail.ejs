<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</head>
<body>

  <a href="/" button type="button" class="btn btn-primary position-absolute top-10 start-50"> Home</a>
  </button>

    <img src="/images/logo.png" style="height: 150px">
        <h1><%= team.Name %> detail</h1>

        <img src="/images/<%= num %>.jpg" style="height: 75px">

    <nav>
    <div class="nav nav-tabs" id="nav-tab" role="tablist">
        <button class="nav-link active" id="nav-ros-tab" data-bs-toggle="tab" data-bs-target="#nav-ros" type="button" role="tab" aria-controls="nav-ros" aria-selected="true">Roster</button>
        <button class="nav-link" id="nav-match-tab" data-bs-toggle="tab" data-bs-target="#nav-match" type="button" role="tab" aria-controls="nav-match" aria-selected="false">Matchup</button>
        <button class="nav-link" id="nav-line-tab" data-bs-toggle="tab" data-bs-target="#nav-line" type="button" role="tab" aria-controls="nav-line" aria-selected="false">Lineup</button>
    </div>
    </nav>
<div class="tab-content" id="nav-tabContent">
  <div class="tab-pane fade show active" id="nav-ros" role="tabpanel" aria-labelledby="nav-ros-tab">

    <table class="table table-striped table-hover table-bordered">
      <tr>
        <td>No.</td>
        <td>Name</td>
        <td>teamId</td>
        <td>Date of birth</td>
        <td>College</td>
        <td>Experience</td>
        <td>Salary</td>
      </tr>
       <% for (let player of players) { %>
      <tr>
        <td><%= player.id %></td>
        <td><%= player.firstName %>  <%= player.lastName %></td>
        <td><%= player.teamId %></td>
        <td><%= player.createdAt %></td>
        <td>unknown</td>
        <td>unknown</td>
        <td>$ <%= player.id %></td>
      </tr>
   <% } %>
      <tr>
        <td>...</td>
        <td>...</td>
        <td>...</td>
        <td>...</td>
        <td>...</td>
        <td>...</td>
        <td>...</td>
      </tr>
    </table>

  </div>
  <div class="tab-pane fade" id="nav-match" role="tabpanel" aria-labelledby="nav-match-tab">

    <table class="table table-striped table-hover table-bordered">
      <tr>
        <td>Date</td>
        <td>Matchup type</td>
        <td>Opponent</td>
        <td>Start <time>  </time></td>
        <td>Result</td>
        <td>Location</td>
        <td>Status</td>
      </tr>
      <tr>
        <td>2016/10/26</td>
        <td>Preseason</td>
        <td>Boston Celtics</td>
        <td>7:30 PM</td>
        <td>112-117</td>
        <td>Chicago</td>
        <td>Finished</td>
      </tr>

      <tr>
        <td>...</td>
        <td>...</td>
        <td>...</td>
        <td>...</td>
        <td>...</td>
        <td>...</td>
        <td>...</td>
      </tr>
    </table>

  </div>
  <div class="tab-pane fade" id="nav-line" role="tabpanel" aria-labelledby="nav-line-tab">

    <img src="/images/line.png  " style="height: 300px">

  </div>
</div>

</div>

</body>
</html>